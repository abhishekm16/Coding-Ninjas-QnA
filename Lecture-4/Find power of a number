import java.util.*;

class Solution {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int n, p;
        n = input.nextInt();
        p = input.nextInt();
        int result = 1;
        if( (n >= 0 && n <= 9) && (p >= 0 && p <= 9)){
            for (int i = 0; i < p; i++) {
                result *= n;
            }
        }
        System.out.println(result);
    }
}
