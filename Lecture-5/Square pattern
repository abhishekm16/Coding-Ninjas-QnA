import java.util.*;

class Solution{
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int n = input.nextInt();
        for (int i = 0; i < n; i++) {
            for(int j=n; j > 0; j--){
                System.out.print(n);
            }
            System.out.println(" ");
        }
    }
}
