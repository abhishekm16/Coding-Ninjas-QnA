import java.util.*;

class Solution {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int n = input.nextInt();
        for(int i=1; i<=n; i++){
            int p = i;
            for(int j=0; j<i; j++){
                System.out.print((p--));
            }
            System.out.println();
        }
    }
}
