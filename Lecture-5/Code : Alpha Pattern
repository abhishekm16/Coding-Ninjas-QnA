import java.util.*;

class Solution {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int n = input.nextInt();
        for(int i=1; i<=n; i++){
            for(int j=1; j<=i; j++){
                System.out.print((char)('A'+i-1));
            }
            System.out.println();
        }
    }
}

Output : 

 A
 BB
 CCC
 DDDD
