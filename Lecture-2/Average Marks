import java.util.Scanner;

public class Solution {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		String str = input.next();
		char ch = str.charAt(0);

		int m1, m2, m3;
		m1 = input.nextInt();
		m2 = input.nextInt();
		m3 = input.nextInt();

		int avg = (m1+m2+m3)/3;
		System.out.println(ch);
		System.out.print(avg);
	}

}
